<view class="uni-searchbar"><view style="{{'border-radius:' + q + ';' + ('background-color:' + r)}}" class="uni-searchbar__box" bindtap="{{s}}"><view class="uni-searchbar__box-icon-search"><block xhs:if="{{$slots.searchIcon}}"><slot name="searchIcon"></slot></block><block xhs:else><uni-icons xhs:if="{{a}}" u-i="7700a72d-0" bind:__l="__l" u-p="{{a}}"/></block></view><input xhs:if="{{b}}" focus="{{c}}" disabled="{{d}}" placeholder="{{e}}" maxlength="{{f}}" class="uni-searchbar__box-search-input" confirm-type="search" type="text" style="{{'color:' + g}}" bindconfirm="{{h}}" bindblur="{{i}}" bindfocus="{{j}}" value="{{k}}" bindinput="{{l}}"/><text xhs:else class="uni-searchbar__text-placeholder">{{m}}</text><view xhs:if="{{n}}" class="uni-searchbar__box-icon-clear" bindtap="{{p}}"><block xhs:if="{{$slots.clearIcon}}"><slot name="clearIcon"></slot></block><block xhs:else><uni-icons xhs:if="{{o}}" u-i="7700a72d-1" bind:__l="__l" u-p="{{o}}"/></block></view></view><text xhs:if="{{t}}" bindtap="{{w}}" class="uni-searchbar__cancel">{{v}}</text></view>